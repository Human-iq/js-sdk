    "chat:shop": "bun run ./demos/chat/shop/index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^5.0.0",
    "chalk": "^5.3.0",
    "express": ">=5.0.1",
    "lowdb": "^7.0.1",
    "openai": "^4.73.0",
    "ora": "^8.1.1",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/*.js",
    "dist/*.d.ts"
  ]
}
