{
  "name": "@humaniq/js",
  "module": "lib/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "build": "bun build --target=node ./lib/index.ts --outfile=dist/index.js && bun run build:declaration",
